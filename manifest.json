{
  "manifest_version": 3,
  "name": "Magnet Link Extractor",
  "version": "1.0.0",
  "description": "Extracts magnet links from the current page.",
  "homepage_url": "https://fnlsg.in",

  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-48.png",
    "64": "icons/icon-128.png"
  },
  "host_permissions": ["*://*/*"],
  "permissions": ["scripting"],
  "action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Magnet Link Extractor",
    "default_popup": "popup/popup.html"
  }
}
